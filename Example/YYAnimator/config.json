    "YYACountingNumberData": {
        "title": "基本数字动画",
        "originProperty" : {"fromValue": 0, "toValue": 0, "numCount": 0},
        "targetProperty" : {"fromValue": 0, "toValue": 100, "numCount": 20},
        "option" : "linear",
        "initialLayout" : "center",
        "time": 10,
        "key" : "countingNumberData",
        "OCValue" : "data",
        "PreCode" : "YYAnimatorCountingNumberData *data = [[YYAnimatorCountingNumberData alloc] init]; data.fromValue = \"fromValue\"; data.toValue = \"toValue\"; int showNumLength = \"numCount\";\ndata.numberFormatBlock = ^NSString * _Nonnull(double numberValue) {\n    int value = ceilf(powf(10, showNumLength) * numberValue);\n    return [NSString stringWithFormat:@\"%0.\"numCount\"f\", value / powf(10, showNumLength)];\n};",
        "SwiftPreCode" : "let data = YYAnimatorCountingNumberData()\ndata.fromValue = \"fromValue\"\ndata.toValue = \"toValue\"\nlet showNumLength = \"numCount\"\ndata.numberFormatBlock = {(numberValue: Double) -> String in\n     let value = ceilf(powf(10, Float(showNumLength)) * Float(numberValue))\n     return String(format: \"%0.\"numCount\"f\", value / powf(10, Float(showNumLength)))\n}",
        "showContent" : "$value",
        "swiftValue": "data"
    },
